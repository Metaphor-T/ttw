#include <stdio.h>
int main() {
    int x;
    scanf("%d", &x);
    int r = 0, n = 0;
    char s[100000];
    int index = 0;
    int first_non_zero = 0;
    
    while (1) {
        r = r * 10 + 1;
        n++;
        int digit = r / x;
        r = r % x;
        if (digit != 0 || first_non_zero) {
            s[index++] = digit + '0';
            first_non_zero = 1;
        }
        if (r == 0) {
            break;
        }
    }
    s[index] = '\0';
    printf("%s %d\n", s, n);
    return 0;
}
