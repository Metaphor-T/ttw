#include<stdio.h>
int main(void){
    int n,x,first=0;
    scanf("%d",&n);
    x=2*n*n+n; //大勾股化简求第一项公式
    printf("%d^2",x);
    for(int i=1;i<=n;i++){
        x++;
        printf(" + %d^2",x);
        }
    printf(" =\n");
    x++;
    printf("%d^2",x);
    for(int j=0;j<n-1;j++){
        x++;
        printf(" + %d^2",x);
    }
    return 0;
    }
