#include <stdio.h>
#define MAXN 20

void CountOff( int n, int m, int out[] );

int main()
{
    int out[MAXN], n, m;
    int i;

    scanf("%d %d", &n, &m);
    CountOff( n, m, out );
    for ( i = 0; i < n; i++ )
        printf("%d ", out[i]);
    printf("\n");

    return 0;
}
void CountOff( int n, int m, int out[] )
{
    int a[n];
    for (int i = 0; i < n; i++) a[i] = 1; //设定1为存活
    int u=1;
    for (int i=0;u<=n;i=(i+1)%n) //不需要拼接数组，可以通过该方法重置
    {
        int flag=0;
        for (int j=i;j<n;j=(j+1)%n)
        {
            if (a[j]==1)
            {
                flag++;
            }
            if (flag==m)
            {
                out[j]=u;
                a[j]=0;
                i=j;
                u++;
                break;
            }
        }
    }
}
